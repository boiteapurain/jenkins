pipeline {
    agent {
        node {
            label 'grosquick'
            }
    }
    triggers {
        pollSCM '*/5 * * * *'
    }
    stages {
        stage('Build') {
            steps {
                echo "Building.."
                sh '''
                echo prout
                '''
            }
        }
        stage('Test') {
            steps {
                echo "Testing.."
                sh '''
                rez env jenkins_test -- python python/fire.py
                '''
            }
        }
        stage('Deliver') {
            steps {
                echo 'Deliver....'
            }
        }
    }
}